<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	style="@style/numpad_wrapper">
	<LinearLayout
		style="@style/numpad_trunk_top">
		<EditText
			android:id="@+id/editPageNum"
			style="@style/numpad_btn_common" />
		<ImageButton
			android:id="@+id/del_btn"
			style="@style/numpad_del_btn" />
		<Button
			android:id="@+id/submit_btn"
			style="@style/numpad_common"
			android:text="确认"/>
	</LinearLayout>
	<LinearLayout
		style="@style/numpad_trunk">
		<Button
			android:id="@+id/num_1_btn"
			style="@style/numpad_btn_common"
			android:text="1"/>
		<Button
			android:id="@+id/num_2_btn"
			style="@style/numpad_btn_common" 
			android:text="2"/>
		<Button
			android:id="@+id/num_3_btn"
			style="@style/numpad_btn_common"
			android:text="3"/>
		<Button
			android:id="@+id/num_4_btn"
			style="@style/numpad_btn_common"
			android:text="4"/>
		<Button
			android:id="@+id/num_5_btn"
			style="@style/numpad_btn_common"
			android:text="5"/>
		<Button
			android:id="@+id/num_6_btn"
			style="@style/numpad_btn_common"  
			android:text="6"/>
		<Button
			android:id="@+id/num_7_btn"
			style="@style/numpad_btn_common"
			android:text="7"/>
		<Button
			android:id="@+id/num_8_btn"
			style="@style/numpad_btn_common" 
			android:text="8"/>
		<Button
			android:id="@+id/num_9_btn"
			style="@style/numpad_btn_common"
			android:text="9"/>
		<Button
			android:id="@+id/num_0_btn"
			style="@style/numpad_btn_common" 
			android:text="0"/>
		<Button
			android:id="@+id/cancel_btn"
			style="@style/numpad_common" 
			android:text="取消"/>
	</LinearLayout>
</LinearLayout>
